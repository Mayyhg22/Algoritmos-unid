Algoritmo calcPromedio
	
	//definimos las 5 calificaciones
	
	Escribir "ingresa la primer nota"
	leer nota1
	Escribir "ingresa la segunda nota"
	Leer nota2
	Escribir "ingresa la tercera nota"
	leer nota3
	Escribir "ingresa la cuarta nota"
	Leer nota4
	Escribir "ingresa la quinta nota"
	leer nota5
	
	Promedio <- (nota1 + nota2 + nota3 + nota4 + nota5)/5;
	
	Escribir "el promedio es: ", promedio;
FinAlgoritmo
